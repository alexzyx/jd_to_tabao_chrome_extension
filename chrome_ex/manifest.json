{
  "name": "JD to Alibaba",
  "version": "1.0",
  "description": "京东收件人信息转淘宝寄件人信息",
  "manifest_version": 2,
  "browser_action": {
      "default_icon": "image/baigei.png",
      "default_title": "拷贝小工具",
      "default_popup": "popup.html"
  },
  "background": {
      "scripts": ["js/background.js"]
  },
  "content_scripts": [
      {
          "matches": ["<all_urls>"],
          "js": ["js/jquery-1.12.4.min.js", "js/content-script.js"]
      }
  ]
}
